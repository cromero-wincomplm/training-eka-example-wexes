<wex-definition>
    <meta>
        <group-id>com.wincomplm</group-id>   
        <artifact-id>wex-demo</artifact-id>
        <display-name>Display name here</display-name>
        <system>false</system>   
        <description>Description here</description>
    </meta>
    <build>
        <version>
            <major>${major}</major>
            <minor>${minor}</minor>
            <beta>${beta}</beta>
            <revision>${revision}</revision>
        </version>
        <host>
            <release>${wex.wt}</release>
        </host>
    </build>
    <install>
        <extension menu="more parts actions" file="PartClient-actionmodels" action="demoHelloWorld"/>
        <extension menu="header actions" file="navigation-actionModels" action="demoHelloWorldNonPop"/>
        <actions>
            <wizard name="demoHelloWorld"   url="demo.jsp" shortcut="false"/>
            <page name="demoHelloWorldNonPop"   url="demo.jsp"/>
        </actions>
    </install>
    <additional>
        <trial/>
        <timestamp>0</timestamp>
        <expires>0</expires>
        <client-id/>
        <client-name/>
        <user-id/>
    </additional>

</wex-definition>